#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/pm2@5.3.1/node_modules/pm2/bin/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/pm2@5.3.1/node_modules/pm2/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/pm2@5.3.1/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/pm2@5.3.1/node_modules/pm2/bin/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/pm2@5.3.1/node_modules/pm2/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/pm2@5.3.1/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/pm2-dev" "$@"
else
  exec node  "$basedir/../../bin/pm2-dev" "$@"
fi
