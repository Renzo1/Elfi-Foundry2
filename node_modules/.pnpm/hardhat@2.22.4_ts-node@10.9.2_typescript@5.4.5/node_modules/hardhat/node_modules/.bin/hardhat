#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/hardhat@2.22.4_ts-node@10.9.2_typescript@5.4.5/node_modules/hardhat/internal/cli/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/hardhat@2.22.4_ts-node@10.9.2_typescript@5.4.5/node_modules/hardhat/internal/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/hardhat@2.22.4_ts-node@10.9.2_typescript@5.4.5/node_modules/hardhat/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/hardhat@2.22.4_ts-node@10.9.2_typescript@5.4.5/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/hardhat@2.22.4_ts-node@10.9.2_typescript@5.4.5/node_modules/hardhat/internal/cli/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/hardhat@2.22.4_ts-node@10.9.2_typescript@5.4.5/node_modules/hardhat/internal/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/hardhat@2.22.4_ts-node@10.9.2_typescript@5.4.5/node_modules/hardhat/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/hardhat@2.22.4_ts-node@10.9.2_typescript@5.4.5/node_modules:/home/krisrenzo/Documents/Audits/Contests/Sherlock/Elfi/2024-05-elfi-protocol/elfi-perp-contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../../internal/cli/bootstrap.js" "$@"
fi
